{
  "name": "front-gestion-vente-locale",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "docker:dev": "docker-compose -f compose.dev.yml up --build",
    "docker:prod": "docker-compose -f compose.prod.yml up --build -d",
    "docker:dev:down": "docker-compose -f compose.dev.yml down",
    "docker:prod:down": "docker-compose -f compose.prod.yml down",
    "docker:dev:logs": "docker-compose -f compose.dev.yml logs -f",
    "docker:prod:logs": "docker-compose -f compose.prod.yml logs -f",
    "test": "npx playwright test --project=chromium"
  },
  "lint-staged": {
    "app/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "pnpm format",
    "preserveUnused": true
  },
